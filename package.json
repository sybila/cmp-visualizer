  "dependencies": {
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "core-js": "^3.6.5",
    "immutable": "^4.0.0-rc.12",
    "plotly.js": "^1.57.1",
    "plotly.js-basic-dist": "^1.57.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-plotly.js": "^2.5.0",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
